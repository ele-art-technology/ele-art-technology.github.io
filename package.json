{
  "name": "parcel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "start": "google-chrome http://localhost:1234/ & parcel index.html",
    "build": "rm -rf dist/* & parcel build index.html --no-minify",
    "build:production": "rm -rf dist/* & parcel build index.html --no-source-maps"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "sass": "^1.19.0",
    "tailwindcss": "^1.0.1"
  }
}
